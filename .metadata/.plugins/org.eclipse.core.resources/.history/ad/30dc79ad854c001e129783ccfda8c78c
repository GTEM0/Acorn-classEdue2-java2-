<script>
	
	let movie = {
		title:"엘리먼트",
		price:11000
	};
	
	console.log(movie);
	
	
	let score ={
		name:"홍길동",
		kor:90,
		eng:90,
		math:90,
		tot:0,
		
		calcTot: function(){
			this.tot = this.kor+ this.eng+ this.math;
		},
		
		hi: function(){
			alert("score 객체 hi~!~!");
		}
				
	}
	score.hi();
	console.log(score.tot);
	score.calcTot();
	console.log(score.tot);
	
</script>



