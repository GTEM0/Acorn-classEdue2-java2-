<script>
	function add1(){
		//alert("dfd1");
		let ul = document.getElementById("fruit_list");
		let li = document.createElement("li");
		li.innerHTML="포도";
		ul.appendChild(li);
	}
	

	function add2(){
		//alert("dfd2");
		$("#fruit_list").append("<li>딸기</li>")
	}
</script>

<body>

	<ul id ="fruti list">
		
		
	</ul>
	
	<button onclick="add1()">과일추가1</button>
	<button onclick="add2()">과일추가2</button>


